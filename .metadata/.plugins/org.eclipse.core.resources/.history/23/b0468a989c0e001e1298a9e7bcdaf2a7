package array_;

import java.util.Scanner;

public class Ex2563 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		int arr[][] = new int[T][2];
		
		int gap = 0;
		for (int i = 0; i < T; i++) {
			arr[i][0] = sc.nextInt(); //x
			arr[i][1] = sc.nextInt(); //y
			
			if (i != 0) {
				int a = arr[i][0];
				int b = arr[i][1];
				for (int j = 0; j < i; j++) {
					if (arr[j][0] > a && arr[j][0] <= a + 10) {
						
					}
				}
			}
		}
		
		sc.close();
	}

}
