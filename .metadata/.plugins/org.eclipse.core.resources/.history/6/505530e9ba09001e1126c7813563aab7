package array_;

import java.util.Scanner;

public class Ex10810 {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		String[] a = sc.next().split("");
		String[] b = sc.next().split("");
		
		String result = "";
		for (int i = 0; i < b.length; i++) {
			result += a[i] + b[i];
		}
		
	}

}
