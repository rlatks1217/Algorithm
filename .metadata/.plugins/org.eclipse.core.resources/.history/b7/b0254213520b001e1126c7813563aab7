		//풀이 2
		Scanner sc = new Scanner(System.in);
		String[] A = sc.next().split("");
		String[] B = sc.next().split("");
		
		String[] biggerOne;
		String[] smallerOne;
		int start = 0;
		
		if(A.length > B.length) {
			biggerOne = A;
			smallerOne = B;
			start = A.length - B.length;
		} else if (A.length == B.length) {
			biggerOne = A;
			smallerOne = B;
		} else {
			biggerOne = B;
			smallerOne = A;
			start = B.length - A.length;
		}
		
		int[] result = new int[biggerOne.length];
		
		for (int i = start; i < biggerOne.length; i++) {
			int a = Integer.parseInt(biggerOne[i]);
			int b = Integer.parseInt(smallerOne[i-start]);
			
			
		}
		
		sc.close();
	}

}
//BigDecimal : long보다 큰 범위의 수 셈할 때 사용
