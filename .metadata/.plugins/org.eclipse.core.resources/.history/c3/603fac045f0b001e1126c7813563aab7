package 입출력과사칙연산;

public class Ex10172 {

	public static void main(String[] args) {
//		|\_/|
//		|q p|   /}
//		( 0 )"""\
//		|"^"`    |
//		||_/=\\__|
		System.out.println("|\\_/|");
		System.out.println("|q p|   /}");
		System.out.println("( 0 )\"\"\"\\");
		System.out.println("|\"^\"`    |");
		System.out.println("||_/=\\\\__|");
	}

}
