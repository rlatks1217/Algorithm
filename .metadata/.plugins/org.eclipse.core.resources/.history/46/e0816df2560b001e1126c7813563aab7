package array_;

import java.math.BigDecimal;
import java.util.Scanner;

public class Ex10810 {

	public static void main(String[] args) {
		// 풀이 1
//		Scanner sc = new Scanner(System.in);
//		BigDecimal A =  new BigDecimal(sc.next());
//		BigDecimal B =  new BigDecimal(sc.next());
//		System.out.println(A.add(B));
//		sc.close();

		// 풀이 2
		Scanner sc = new Scanner(System.in);
		String[] A = sc.next().split("");
		String[] B = sc.next().split("");

		String[] biggerOne;
		String[] smallerOne;
		boolean differ = false;
		int start = 0;

		if (A.length > B.length) {
			biggerOne = A;
			smallerOne = B;
			differ = true;
			start = A.length - B.length;
		} else if (A.length == B.length) {
			biggerOne = A;
			smallerOne = B;
		} else {
			biggerOne = B;
			smallerOne = A;
			differ = true;
			start = B.length - A.length;
		}

		int[] result = new int[biggerOne.length + 1];

		if (differ) {
			for (int i = 0; i < start; i++) {
				result[i + 1] = Integer.parseInt(biggerOne[i]);
			}
		} else {
			int plus = Integer.parseInt(biggerOne[0]) + Integer.parseInt(smallerOne[0]);
			if (plus > 9) {
				result[0] = plus/10;
				result[1] = plus%10;
				start = 1;
			}
		}

		for (int i = start; i < biggerOne.length; i++) {
			int a = 0;
			int b = 0;
			if (biggerOne.length != smallerOne.length) {
				a = Integer.parseInt(biggerOne[i]);
				b = Integer.parseInt(smallerOne[i - start]);
			} else {
				a = Integer.parseInt(biggerOne[i]);
				b = Integer.parseInt(smallerOne[i]);				
			}

			int sum = a + b;
			
			System.out.println(sum);
			
			if (sum > 9) {
				result[i] += sum/10;
				int cnt = i; //하나 위의 자리
				while(result[cnt] > 9) {
					result[cnt] = 0;
					cnt--;
					result[cnt] += 1;
				}
				result[i+1] += sum%10;
			} else {
				result[i+1] += sum;				
			}
		}
		
		String answer = "";
		if (result[0] > 0) {
			answer += result[0];
		}
		
		for (int i = 1; i < result.length; i++) {
			answer += result[i];
		}

		System.out.println(answer);
		
		sc.close();
	}

}
//BigDecimal : long보다 큰 범위의 수 셈할 때 사용
