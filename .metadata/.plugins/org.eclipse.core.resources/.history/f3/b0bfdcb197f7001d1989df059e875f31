package _programers;

public class 최댓값과최솟값 {

	public static void main(String[] args) {
		String s = "1 2 3 4";
		
		System.out.println(solution(s));
	}

	private static String solution(String s) {
		
		int min = Integer.MAX_VALUE;
		int max = Integer.MIN_VALUE;
		
		for (int i = 0; i < s.length(); i++) {
			int a = s.charAt(i) - '0';
			min = Math.min(min, a);
		}
		return null;
	}

}
