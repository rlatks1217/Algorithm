package _programers;

public class 소수만들기 {

	public static void main(String[] args) {
		
		int[] nums = {1,2,3,4};
		
		int max = 0;
		for (int i = 0; i < nums.length; i++) {
			if (max <= nums[i]) {
				max = nums[i];
			}
		}
		
		boolean[] sosoo = new boolean[max+1];
		
		for (int i = 0; i < nums.length; i++) {
			
		}
	}

}
