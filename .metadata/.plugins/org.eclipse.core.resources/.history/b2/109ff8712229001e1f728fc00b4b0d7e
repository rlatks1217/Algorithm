package 직사각형과삼각형;

import java.util.Scanner;

public class Ex14215 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		int max = Math.max(a, b);
		max = Math.max(max, c);
		
		switch (max) {
		case a: 
			
			break;
		case b: 
			
			break;
		case c: 
			
			break;
	}

}
//해론의 공식 참고
