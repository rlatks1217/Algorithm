package array_;

import java.util.Scanner;

public class Ex2563 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int T = sc.nextInt();
		
		int arr[][] = new int[T][2];
		
		int gap = 0;
		for (int i = 0; i < T; i++) {
			int x = sc.nextInt(); //x
			int y = sc.nextInt(); //y
			
			arr[i][0] = x; 
			arr[i][1] = y;
			
			if (i > 0) {
				int x1 = x;
				int x2 = x + 10;
				int y1 = y;
				int y2 = y + 10;
				for (int j = 0; j < i; j++) {
					int a = arr[j][0]; //x
					int b = arr[j][1]; //y
					
					if (x1 <= a + 10 && x1 > a) {
						if (y1 > b && y1 <= b + 10) {
							gap += ((a + 10) - x1)*((b + 10) - y1);
						} else if (y2 > b && y2 <= b + 10) { 
							
						}
					} else if (x2 <= a + 10 && x2 > a) {
						
					}
					
					
				}
			}
		}
		
		sc.close();
	}

}
