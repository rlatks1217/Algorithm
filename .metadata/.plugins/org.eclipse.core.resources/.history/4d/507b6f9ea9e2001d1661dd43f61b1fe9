package _programers;

public class 명예의전당 {

	public static void main(String[] args) {
		
		int k = 3;
		int[] score = {10,100,20,150,1,100,200};
		
		//명예의 전당
		int[] mj = new int[k];
		
		//그날 최하위 친구 점수
		int[] result = new int[7];
		
		for (int i = 0; i < 7; i++) {
			if (i < k) {
				mj[i] = score[i];
				if(i == 0) {
					result[i] = score[i];
				}
			}
		}
		
	}

}
