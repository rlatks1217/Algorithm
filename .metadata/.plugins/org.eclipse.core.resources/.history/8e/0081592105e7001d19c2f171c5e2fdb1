package _programers;

public class 로또의최고순위와최저순위 {

	public static void main(String[] args) {
		
		int lottos[] = {44,1,0,0,31,25};
		int win_nums[] = {31,10,45,1,6,19};
		
		int zeroCount = 0;
		int absolute = 0;
		
		for (int i = 0; i < 6; i++) {
			if (lottos[i] == 0) {
				zeroCount++;
				continue;
			}
			for (int j = 0; j < 6; j++) {
				if (lottos[i] == win_nums[j]) {
					absolute++;
				}
			}
			
			System.out.println("헤헤헤ㅔ");
		}
	}

}
