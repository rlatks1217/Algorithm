    public static int solution(String numbers) {
    	int answer = 0;
    	
    	Set<Integer> set = new HashSet<>();
    	permutation("", numbers, set);
    	
    	for (int num : set) {
			if (isPrime(num)) {
				answer++;
			}
		}
    	
    	return answer;
    }

