public class 개인정보수집유효기간 {

	public static void main(String[] args) {
		String today = "2022.05.19";
		String[] term = {"A 6", "B 12", "C 3"};
		String[] privacies = {"2021.05.02 A", "2021.07.01 B", "2022.02.19 C", "2022.02.20 C"};
		
//		today = today.replace(".", "-");
//		LocalDate date =  LocalDate.parse(today);
//		
//		System.out.println(date.plusMonths(5).isAfter(date));
		System.out.println(solution(today, term, privacies));
	}

	private static int[] solution(String today, String[] term, String[] privacies) {
		int[] answer = {};
		
		today = today.replace(".", "-");
		LocalDate date =  LocalDate.parse(today);
		
		ArrayList<String[]> list = new ArrayList<>();
		ArrayList<String[]> termList = new ArrayList<>();
		
		for (int i = 0; i < privacies.length; i++) {
			String[] a = privacies[i].split(" ");
			list.add(a);
		}
		
		for (String t : term) {
			String[] a = t.split(" ");
			termList.add(a);
		}
		
		for (int i = 0; i < list.size(); i++) {
			
		}
		
		
		return null;
	}

}
