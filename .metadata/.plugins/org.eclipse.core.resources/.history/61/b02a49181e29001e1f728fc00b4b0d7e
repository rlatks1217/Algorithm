package 직사각형과삼각형;

import java.util.Scanner;

public class Ex14215 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		
		if (a == b) {
			if (b == c) {
				System.out.println("Equilateral");
			} else {
				System.out.println("Isosceles");				
			}
		} else if (b == c) {
			System.out.println("Isosceles");
		} else if (a == c) {
			System.out.println("Isosceles");			
		} else {
			System.out.println("Scalene");						
		}
		
	}

}
