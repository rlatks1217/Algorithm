package _programers;

public class JadenCase {

	public static void main(String[] args) {
		String s = "3people unFollowed me";
		System.out.println(solution(s));
	}

	private static String solution(String s) {
		String answer = "";
		String a = s.toUpperCase();
		
		System.out.println(a);
		
		return answer;
	}

}
